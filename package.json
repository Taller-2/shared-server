{
  "name": "shared-server",
  "version": "0.0.1",
  "description": "Shared server del TP de Taller II 2018/2",
  "main": "server",
  "engines": {
    "node": "8.9.4"
  },
  "scripts": {
    "client": "cd client && npm run start",
    "server": "nodemon server",
    "server-debug": "nodemon --inspect server",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "debug": "concurrently --kill-others-on-fail \"npm run server-debug\" \"npm run client\"",
    "start": "node server",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "test": "npm run beforetest && mocha",
    "beforetest": "eslint ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "mocha": "^5.2.0",
    "mysql": "^2.16.0",
    "nodemon": "latest",
    "pg": "^7.4.3",
    "pg-hstore": "latest",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "concurrently": "^4.0.0",
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Taller-2/shared-server.git"
  }
}
