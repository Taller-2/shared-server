{
  "name": "shared-server",
  "version": "0.0.1",
  "description": "Shared server del TP de Taller II 2018/2",
  "main": "server",
  "scripts": {
    "client": "cd client && npm run start",
    "server": "nodemon server",
    "server-debug": "nodemon --inspect server",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "debug": "concurrently --kill-others-on-fail \"npm run server-debug\" \"npm run client\"",
    "start": "node server",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "test": "mocha"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.1.2",
    "express": "^4.16.3",
    "mocha": "^5.2.0",
    "mysql": "^2.16.0",
    "nodemon": "latest",
    "pg": "^7.4.3",
    "pg-hstore": "latest",
    "sequelize": "^4.38.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Taller-2/shared-server.git"
  }
}
